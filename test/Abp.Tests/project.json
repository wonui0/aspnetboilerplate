{
  "version": "1.0.0-*",
  
  "testRunner": "xunit",

  "dependencies": {
    "Abp": "1.5.0.0-*",
    "Abp.TestBase": "1.5.0.0-*",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "NSubstitute": "2.0.2",
    "Shouldly": "2.8.2",
    "xunit": "2.2.0-beta2-build3300"
  },

  "frameworks": {
    "net452": {
      "Microsoft.NETCore.App": "1.1.0"
    }
  },

  "buildOptions": {
    "embed": {
      "include": [
        "Localization/Json/JsonSources/*.json",
        "Localization/Json/XmlSources/*.xml",
        "Localization/TestResourceFiles/*.resx",
        "Localization/TestXmlFiles/*.xml",
        "Resources/Embedded/MyResources/js/*.js"
      ]
    }
  }
}
